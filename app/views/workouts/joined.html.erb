<h1 class="m-3"> <%=User.where({:id=>session.fetch(:user_id)}).first.username%>'s' Joined Workouts </h1>

<table class="table table-bordered m-3" border="1">
    <thead class="thead-dark">
      <tr>

        <th scope="col">
          Workout
        </th>


      </tr>
      </thead>
      <% @list_of_joins.where(:user_id=>session.fetch(:user_id)).each do |a_join|%>
      <tbody>
      <tr>

        <td>
          <a class="btn btn-outline-success" href="/workouts/<%=a_join.workout_id%>">
            Show details
          </a>
        </td>
      </tr>
      </tbody>
      <% end %>
    </table>
